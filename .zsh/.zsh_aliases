## iOS aliases
alias poi="pod install"
alias pou="pod update"
alias podfix="pod deintegrate && pod install"
alias clear_derived="fastlane run clear_derived_data"
alias clean_ios="podfix && clear_derived"

## Git aliases
alias gcl="git clone"
alias gl="git pull"
alias gp="git push"
alias gco="git checkout"
alias gsta="git stash"
alias gstaa="git stash apply"
alias gcaam="git add . && gc -m"
alias glo="git pull origin"
alias glom="git pull origin master"
alias glod="git pull origin develop"
alias glode="git pull origin development"
alias glum="git pull upstream master"
alias glud="git pull upstream develop"
alias glude="git pull upstream development"
alias gres="git reset HEAD"
alias gcem="git commit --allow-empty -m"
alias gcom="git checkout master && git pull"
alias gcod="git checkout develop && git pull"
alias gcode="git checkout development && git pull"
alias gst="git status"
alias gs="git status"
alias gd="git diff"
alias gclean="git clean -fd"
